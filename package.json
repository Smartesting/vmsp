  "type": "module",
  "types": "dist/types.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test": "jest",
    "start": "ts-node src/index.ts",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
    "pretty-quick": "pretty-quick",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [],
  "author": "sma",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bitset": "^5.1.1"
  },
  "repository": {
